# MedNex Backend Environment Variables

# Groq API Configuration
GROQ_API_KEY=your_groq_api_key_here

# Hugging Face Configuration (Optional - for additional models)
HUGGINGFACE_TOKEN=your_hf_token_here

# Database Configuration (Optional - uses fallback if not provided)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# API Configuration
PORT=8000
HOST=0.0.0.0

# CORS Configuration  
CORS_ORIGINS=http://localhost:3000,https://mednex-frontend.onrender.com

# Dataset Configuration
DATASET_PATH=./data/disease_symptom_dataset.csv

# Development Settings
DEBUG=True
LOG_LEVEL=INFO

# Model Configuration
BIOBERT_MODEL_NAME=dmis-lab/biobert-v1.1
LLAMA_MODEL_NAME=llama-3.2-90b-text-preview

# Cache Configuration
MODEL_CACHE_DIR=./models/cache
